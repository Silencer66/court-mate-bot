{
  "name": "court-mate-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ./env/development.env -- nodemon --ext ts --exec node --inspect --no-deprecation -r esbuild-register src/index.ts",
    "build": "node scripts/build.js",
    "start": "dotenv -e ./env/production.env -- node -r module-alias/register dist/index.js",
    "start-dev": "dotenv -e ./env/development.env -- node -r module-alias/register dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "dotenv -e ./env/development.env -- prisma db push",
    "db:push-prod": "dotenv -e ./env/production.env -- prisma db push",
    "db:migrate": "dotenv -e ./env/development.env -- prisma migrate dev",
    "db:migrate-prod": "dotenv -e ./env/production.env -- prisma migrate deploy",
    "db:studio": "dotenv -e ./env/development.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "dotenv": "^17.2.1",
    "module-alias": "^2.2.3",
    "prisma": "^6.14.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.25.9",
    "esbuild-plugin-tsconfig-paths": "^1.0.1",
    "esbuild-register": "^3.6.0",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.17"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
